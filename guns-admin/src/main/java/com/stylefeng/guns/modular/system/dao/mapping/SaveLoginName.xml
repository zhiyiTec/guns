<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.stylefeng.guns.modular.system.dao.SaveLoginNameMapper">
	<select id="SaveLoginNameMapper" resultType="String">
	select name from saveLoginName
	</select>
	<update id="updateLoginName" parameterType="com.stylefeng.guns.modular.system.model.SaveLoginName">
	update saveLoginName set name=#{name} where id=#{id}
	</update>
	
</mapper>
